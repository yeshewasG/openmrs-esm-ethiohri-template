{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "fhir2": ">=1.2",
    "webservices.rest": "^2.24.0"
  },
  "extensions": [
    {
      "name": "Red box",
      "component": "redBox",
      "slot": "Boxes"
    },
    {
      "name": "Blue box",
      "component": "blueBox",
      "slot": "Boxes"
    },
        {
      "name": "Brand box",
      "component": "blueBox",
      "slot": "Boxes"
    },
    {
      "name": "template-esm-ext",
      "slot": "patient-chart-dashboard-slot",
      "component": "templateEsmMenu"
    },
    {
      "name": "template-esm-service-ext",
      "slot": "template-esm-slot",
      "component": "templateEsmDashboardLink",
      "meta": {
        "title": "Template-ESM-Service",
        "slot": "template-esm-service-slot",
        "path": "template-esm-service",
        "columns": 1
      }
    },
    {
      "name": "teplate-esm-summary-widget",
      "component": "transferOutSummary",
      "slot": "template-esm-dashboard-slot",
      "online": true,
      "offline": true,
      "meta": {
        "columnSpan": 4
      }
    },
    {
      "name": "template-esm-chart-ext",
      "slot": "template-esm-service-slot",
      "component": "templateEsmServiceChart"
    },
    {
      "name": "teplate-esm-dashboard",
      "component": "transferOutDashboardLink",
      "slot": "patient-chart-dashboard-slot",
      "online": true,
      "offline": true,
      "order": 6,
      "meta": {
        "columns": 1,
        "slot": "template-esm-dashboard-slot",
        "path": "template-esm"
      }
    },
    {
      "name": "encounter-delete-confirmation-dialog",
      "component": "encounterDeleteConfirmationDialog"
    }
  ],
  "pages": [
    {
      "component": "root",
      "route": "root"
    }
  ],
  "workspaces": [
    {
      "name": "template-esm-workspace",
      "title": "Template ESM",
      "component": "templateEsmWorkspace",
      "type": "form",
      "canMaximize": false,
      "canHide": false,
      "width": "wider"
    }
  ]
}
